{
    "name": "hexor/lf-package-manager",
    "description": "Package manager for Laravel 5+.",
    "keywords": ["framework", "laravel", "composer", "automation", "laravel-package"],
    "license": "MIT",
    "homepage": "https://permafrost-dev.com/laravel-package-manager/",
    "authors": [
        {
            "name": "Patrick Organ",
            "email": "patrick@permafrost-software.com"
        },
        {
            "name": "Hexor Chang",
            "email": "cupsoda@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "symfony/console": "~3.2",
        "symfony/process": "~3.2"
    },
    "require-dev": {
        "mockery/mockery": "~0.9.4",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "LaravelPackageManager\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },

    
    "minimum-stability": "dev",
    "prefer-stable": true
}
